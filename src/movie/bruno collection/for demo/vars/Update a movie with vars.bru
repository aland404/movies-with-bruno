meta {
  name: Update a movie with vars
  type: http
  seq: 3
}

put {
  url: {{baseUrl}}/movies/{{slug}}
  body: formUrlEncoded
  auth: none
}

body:form-urlencoded {
  slug: {{slugWithVar}}
  title: {{titleWithVar}}
}

vars:pre-request {
  slugWithVar: a-nightmare-on-elm-street
  titleWithVar: A unicorn in the garden
}
